<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.ps.Fallo_registro" style="-fx-background-image: url('images/Con2.png'); -fx-background-size: cover; -fx-background-repeat: no-repeat;">
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
    <Text text="NO SE HA PODIDO REALIZAR EL REGISTRO" styleClass="h1"/>
    <ImageView fitWidth="200" fitHeight="200">
      <image>
         <Image url="@/images/error.png" />
      </image>
   </ImageView>
    <Button fx:id="jugarBJ" text="VOLVER" styleClass="btn-primary, btn-lg" maxWidth="250" maxHeight="250" onAction="#irAMenu" />
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>

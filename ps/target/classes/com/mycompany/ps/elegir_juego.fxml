<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.ps.ElegirJuego" style="-fx-background-color: lightblue;">
    <children>
        <Button fx:id="jugar" text="JUGAR EN LOCAL" styleClass="btn-primary, btn-lg" prefWidth="400" prefHeight="50" onAction="#jugarLocal" />
        <Button fx:id="perfil" text="JUGAR EN LÍNEA" styleClass="btn-primary, btn-lg" prefWidth="400" prefHeight="50" onAction="#jugarOnline" />
        <Button fx:id="volver" text="VOLVER" styleClass="btn-primary, btn-lg" prefWidth="400" prefHeight="50" onAction="#volver" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
